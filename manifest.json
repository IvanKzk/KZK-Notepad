{
    "manifest_version": 2,
 
    "name": "KZK Notepad",
    "description": "",
    "version": "1.0",
 
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "starter.html"
    },

    "permissions": [
		"storage",
    "https://*/"
    ],
	
	"background": {
		"scripts": ["plugins/jquery-3.4.1.js","plugins/biri.min.js", "definitions.js", "background.js"],
		"persistent": false
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';"
}	